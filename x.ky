(defmacro poo [a b & more]
`(.add ~a ~b ~@more))

(defn xmod
  "Return the proper mathematical modulo of x mod N."
  [x N]
  (if (neg? x)
    (- x (* -1 (+ N (* N (Math/floor (/ (- x) N)))))) (rem x N)))

(defn xxx [a b]
