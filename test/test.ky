(ns ^{:doc ""
      :author "Kenneth Leung"}
  czlab.kirby.test.stdlib)

;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
(deftest xxx

  (ensure (not (empty? [1])))
  (ensure (empty? []))
  (ensure (starts? "abc" "a"))
  (ensure (ends? "abc" "c"))
  (ensure (= 3 (n# [1 2 3])))
  (ensure (= 1 (1st [1 2])))
  (ensure (= 2 (2nd [1 2])))
  (ensure (= 3 (3rd [1 2 3])))

  (ensureThrown (trap! "hello" "world"))
  (ensure (= "hello" (.-message (merror "hello")))))





